ing    = "_OscSyncProcessError";		/** 同期プロセスが破棄された際に通知されます. */        public static const DESTROY:String  = "_OscSyncProcessDestroy";				/** Group に登録が受け付けられた際に通知される. */		public static const ACTIVATE:String = "_OscSyncProcessActivate";		        //------- MEMBER ----------------------------------------------------------------------- */                //------- PUBLIC ----------------------------------------------------------------------- */				/**		 * Constructor.		 * @param type		 * @param bubbles		 * @param cancelable		 */        public function OSCSyncProcessEvent( type:String, bubbles:Boolean=false, cancelable:Boolean=false ) { 			super(type, bubbles, cancelable);        }                public override function clone():Event {            return new OSCSyncProcessEvent( type, bubbles, cancelable );        }                public override function toString():String {             return formatToString("OscSyncProcessEvent", "type", "bubbles", "cancelable", "eventPhase");         }		        //------- INTERNAL --------------------------------------------------------------------- */            }	}